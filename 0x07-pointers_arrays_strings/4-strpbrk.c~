#include "holberton.h"
#include <stdio.h>

/**
 * main - check the code for Holberton School students.
 *
 * Return: Always 0.
 */
char *_strpbrk(char *s, char *accept)
{
  int a = 0;
  while (*s != 0)
    {
      if (*s == *accept)
	{
	  a = 1;
	  break;
	}
      s++;
    }
  if (a != 0)
    return (s);
  else 
    return (0);
}
